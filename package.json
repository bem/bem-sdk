{
  "name": "bem-graph",
  "version": "0.0.0",
  "description": "Bem graph storage",
  "license": "MPL-2.0",
  "author": "Andrew Abramov <mail@blond.im>",
  "keywords": [
    "bem",
    "graph",
    "entity",
    "successors",
    "dependencies"
  ],
  "repository": "bem-sdk/bem-graph",
  "engines": {
    "node": ">= 4"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "dependencies": {
    "bem-entity-name": "0.0.2",
    "bem-naming": "1.0.1",
    "es6-error": "3.0.0",
    "hash-set": "1.0.0",
    "ho-iter": "0.3.0",
    "lodash": "4.12.0"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "eslint": "^3.2.2",
    "eslint-config-pedant": "^0.7.0",
    "nyc": "^6.6.1",
    "sinon": "^1.17.4"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "nyc ava",
    "lint": "eslint ./lib && cd spec && eslint . && cd ../test && eslint .",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "ava": {
    "require": "./test/.ava.js",
    "files": [
      "spec/**/*.spec.js",
      "test/**/*.test.js"
    ],
    "source": [
      "lib/**/*.js"
    ],
    "concurrency": 25
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
